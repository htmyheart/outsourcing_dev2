<script type="text/javascript">
    // <![CDATA[
    require([
        'jquery'
    ], function ($) {

        setTimeout(function () {
            $('body').find('select option[value=""]').each(function(){
                if (!$(this).text().trim().length) {
                    $(this).remove();
                }
            });
        }, 5000);

    });
    // ]]>
</script>

